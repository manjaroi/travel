.form_set {
    padding-left: 76px;
    line-height: 30px;
}
.plans .form_input {
    margin-bottom: 4px;
}
.form_set .form_label {
    float: left;
    width: 76px;
    margin: 0 0 12px -76px;
    white-space: nowrap;
    color: #999;
    text-align: left;
    padding-right: 25px;
}

.form_set .form_input {
    position: relative;
    float: left;
    width: 100%;
    margin: 0 0 12px;
}


.plans .plan_item {
    overflow: hidden;
    position: relative;
    float: left;
    max-width: 40%;
    height: 30px;
    margin: 0 8px 8px 0;
    padding: 1px 10px;
    border: 1px solid #ccc;
    background: #fff;
    color: #333;
    line-height: 28px;
    font-size: 14px;
    white-space: nowrap;
    text-overflow: ellipsis;
   
    cursor: pointer;
}
.plans .checked {
    padding: 0 9px;
    border: 2px solid #F86A5C;
}
.plans .checked i{
    display:block;
    width: 14px;
    height: 14px;
    background:url(../imgs/d11.png) no-repeat;
    position: absolute;
    right: -2px;
    bottom: -1px;
    
}
 .date_select {
    position: relative;
    height: 30px;
    padding: 0 20px 0 8px;
    border: 1px solid #ccc;
   
}
.date_select {
    width: 400px;
    background: #fff;
    color: #333;
    font: 14px/28px 'Microsoft Yahei UI','Microsoft Yahei',Menlo,Monaco,Consolas,'Courier New';
    white-space: nowrap;
    cursor: pointer;
}
.counters {
    position: relative;
    white-space: nowrap;
}
.counters .label {
    margin-left: 10px;
   
}
.counters .counter,.counters .label {
    float: left;
}


.counter {
    position: relative;
}
.counter .ctrl.disabled,.counter .ctrl.disabled:hover {
    color: #dcdcdc;
    cursor: not-allowed;
}
.counters .counter_item-child {
    margin-left: 36px;
}
.counters .counter_item {
    position: relative;
    float: left;
}
.counter .ctrl {
    width: 28px;
    background: #f5f5f5;
    border: 1px solid #ccc;
    text-decoration: none;
    line-height: 22px;
    font-size: 22px;
    color: #666;
}
.counter .num {
    width: 42px;
    border-top: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
    border-left: none;
    border-right: none;
    vertical-align: middle;
    line-height: 24px;
    font-size: 14px;
    color: #333;
}
.counter .ctrl,.counter .num {
    float: left;
    height: 28px;
    margin: 0;
    padding: 0;
    outline: 0;
    text-align: center;
}
.order_prices {
    margin-top: 4px;
    margin-bottom: 11px;
}
.order_prices .name {
    display: inline-block;
    width: 82px;
    padding-right: 25px;
    color: #999;
}
.order_prices .total {
    font-size: 18px;
    color: #F86A5C;
}
.placeOrder {
    display:block;
    width: 140px;
    height: 44px;
    background: #F86A5C;
    text-align: center;
    line-height: 44px;
    font-size: 20px;
    font-weight: bolder;
    color: #fff;
    cursor: pointer;
}











