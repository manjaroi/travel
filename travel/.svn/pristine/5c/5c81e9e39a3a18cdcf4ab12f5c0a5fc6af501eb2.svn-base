#l_foot{
	width: 100%;
	border-top: 3px solid #58bc58;
	font-size: 12px;
}
.l_foot_l_l{
	float: left;
}
.l_foot_l_l dl{
	width: 60px;
	float: left;
	padding-top: 26px;
	margin-left: 87px;
}
.l_foot_l_l dl.first{
	margin-left: 0;
}
.l_foot_l_l dt{
	font-size: 14px;
}
.l_foot_l_l dd{
	color: #666666;
}
.l_foot_l_r{
	padding-top: 23px;
	float: left;
	margin-left: 87px;
}
.l_foot_l_r p:nth-child(1){
	color: #ccc;
}
.l_foot_l_r p:nth-child(2){
	font-size: 20px;
	color: #58bc58;
	margin: 5px 0px;
}
.l_foot_l_r p:nth-child(3){
	margin: 5px 0px;
}
.l_foot_l_r a{
	display: block;
	width:117px;
	height: 31px;
	color: #fff;
	text-align: center;
	line-height: 31px;
	background-color: #00AF67;
}
.l_foot_b{
	margin-top: 40px;
}
.l_foot_b div{
	float: left;
	width: 136px;
	height: 40px;
	border: 2px solid #F1F1F1;
	margin-right: 10px;
	color: #929090;
}
.l_foot_b i.icon{
	float: left;
	display: inline-block;
	width: 60px;
	height: 40px;
}
.l_foot_b i.icon1{	
	background: url(../imgs/l_100.png) -170px 0 no-repeat;
}
.l_foot_b i.icon2{	
	background: url(../imgs/l_100.png) -250px 0 no-repeat;
}
.l_foot_b i.icon3{	
	background: url(../imgs/l_100.png) -320px 0 no-repeat;
}
.l_foot_b i.icon4{	
	background: url(../imgs/l_100.png) -380px 0 no-repeat;
}
.l_foot_b i.icon5{	
	background: url(../imgs/l_100.png) -430px 0 no-repeat;
}
.l_foot_b i.icon6{	
	background: url(../imgs/l_100.png) -490px 0 no-repeat;
}
.l_foot_b i.icon7{	
	background: url(../imgs/l_100.png) -550px 0 no-repeat;
}
.l_foot_b div span.last{
	display: inline-block;
	width: 70px;
	height: 38px;
	background-color: #BE0411;
	color: #fff;
	text-align: center;
	line-height: 38px;
	border-radius: 5px;
}
.l_foot_p{
	margin-top: 40px;
	float: left;
	color: #A6B5B3;
}
.l_logo{
	margin-top: 26px;
}
.l_logo p{
	color: #666666;
	margin-top: 10px;
}