#l_main{
	width: 100%;
	color: #333333;
	font-size: 12px;
	/*height: 2000px;*/
}
#l_main .container{

}
#l_main .l_left{

}
#l_main .l_top{
	padding-top: 17px;
}
#l_main .l_top a{
	color: #666666;
}
#l_main .l_top a .arrow{
	display: inline-block;
	width: 0px;
	height: 0px;
	border-width: 3px;
	border-style: solid;
	border-color: #666 transparent transparent transparent;
	margin-left: 2px;
	margin-right: 3px;
}
.l_route{
	width: 905px;
	margin-top: 23px;
	border: 1px solid #ccc;
	padding-bottom: 18px;
}
.l_all{
	width: 100%;
	height: 40px;
	background-color: #F8F8F8;
}
.l_all a{
	display: block;
	float: left;
	width: 107px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #333333;
}
.l_all a.first{
	background-color: #fff;
	border-top: 2px solid #58bc58;
}
.l_all a:hover{
	color:#58bc58;
}
.l_city .first{
	padding-top: 30px;
}
.l_city p{
	padding-top: 20px;
}
.l_city span{
	display: inline-block;
	width: 54px;
	height: 15px;
	margin-left: 18px;
	color: #999999;
	text-align: right;
}
.l_city b{
	color: #58bc58;
	margin-left: 25px;
}
.l_city a{
	margin-left: 20px;
	color: #333;
}
.second a b{
	margin-left: 0px;
	margin-right: 5px;
}
.l_open{
	padding-top: 20px;
}
.l_open i{
	display: inline-block;
	border-style: solid;
	width: 7px;
	height: 7px;
	border-right: 3px;
	border-bottom: 3px;
	border-color: #666;
	margin-left: 5px;
	margin-right: 3px;
	margin-bottom: 2px;
	transform: rotate(-135deg);
}
.l_general{
	clear: both;
}
.l_general a{
	color: #666666;
	display: inline-block;
	margin: 0 5px;
}
.l_general a.first{
	color: #58bc58;
}
.l_general span{
	width: 10px;
	display: inline-block;
}
.l_general .arrow1{
	display: inline-block;
	border-style: solid;
	border-color: transparent transparent #666 transparent;
	margin-left: 2px;
	margin-right: 3px;
	margin-bottom: -10px;
}
.l_general .arrow2{
	display: inline-block;
	border-style: solid;
	border-color: #666 transparent transparent transparent;
	margin-left: 2px;
	margin-right: 3px;
	margin-bottom: -2px;
}
.l_right{
	padding-top: 56px;
	padding-left: 12px;
}
.l_right .second{
	margin-top: 10px;
	width: 282px;
	height: 237px;
	border: 1px solid #ccc;
	border-top: 3px solid #58bc58;
}
.l_right .l_relax{
	height: 75px;
	width: 100%;
	border-bottom: 1px solid #ccc;
}
.l_right .l_relax p:nth-child(1) {
	float: left;
	margin-left: 30px;
}
.l_right .l_relax p i {
	/*float: left;*/
	display: inline-block;
	width: 38px;
	height: 38px;
	margin-top: 17px;
	background: url(../imgs/l_100.png) 0 0 no-repeat;
}
.l_right .l_relax p:nth-child(2){
	float: left;
	margin-top: 12px;
	margin-left: 5px;
}
.l_right .l_relax p:nth-child(2) span:nth-child(1){
	font-size: 20px;
	color: #66666B;
}
.l_right .l_relax p:nth-child(2) span:nth-child(3){
	color: #999;
}
.l_real{
	/*text-align: center;*/
	margin-left: 30px;
}
.l_real p{
	padding-top: 15px;
}
.l_real .first{
	padding-top: 20px;
}
.l_real p span{
	color: #666666;	
	margin-left: 6px;
}
.l_real .icon1{
	display: inline-block;
	width: 18px;
	height: 18px;
	vertical-align: middle;
	background: url(../imgs/l_100.png) -45px 0 no-repeat;
}
.l_real .icon2{
	display: inline-block;
	width: 18px;
	height: 18px;
	vertical-align: middle;
	background: url(../imgs/l_100.png) -70px 0 no-repeat;
}
.l_real .icon3{
	display: inline-block;
	width: 18px;
	height: 18px;
	vertical-align: middle;
	background: url(../imgs/l_100.png) -100px 0 no-repeat;
}
.l_real .icon4{
	display: inline-block;
	width: 18px;
	height: 18px;
	vertical-align: middle;
	background: url(../imgs/l_100.png) -130px 0 no-repeat;
}
.l_right .third{
	width: 280px;
	/*height: 284px;*/
	border: 1px solid #ccc;
	margin-top: 10px;
}
.l_right .third p{
	margin-top: 12px;
}
.l_right .third .block{
	margin-top: 12px;
	margin-left: 8px;
	margin-right: 8px;
	padding-bottom: 10px;
	border-bottom: 1px solid #ccc;
}
.l_right .third div.third_r{
	width: 137px;
	float: left;
	margin-left: 20px;
}
.l_right .third div img{
	display: inline-block;
	width: 95px;
	height: 60px;
	margin-left: 10px;
	float: left;
}
.l_right .third div span{
	display: block;
	/*float: left;*/
}
.l_right .third div.third_r span:nth-child(1){
	width: 137px;
	height: 32px;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp:2;
	-webkit-box-orient:vertical;
}
.l_right .third div.third_r span:nth-child(2){
	float: right;
	margin-top: 10px;
}
.l_right .third div.third_r span:nth-child(2) i{
	color: #FF811A;
	margin-right: 5px;
}
.l_right .third .last{
	border-bottom: none;
}



/*================= 后端生成数据css ================*/	
.l_content{
	margin-top: 10px;
	width: 904px;
	height: 156px;
	border: 1px solid #ccc;
}
.l_content img{
	display: inline-block;
	float: left;
	width: 200px;
	height: 133px;
	margin: 12px;
}
.l_content .l_content_r{
	margin-top: 10px;
	float: left;
}
.l_content .l_content_r .l_content_r_c{
	width: 500px;
}
.l_content .l_content_r .l_content_r_c p:nth-child(1) {
	font-size: 16px;
}
.l_content .l_content_r .l_content_r_c p:nth-child(2) {
	margin-top: 10px;
}
.l_content .l_content_r .l_content_r_c p:nth-child(2) span{
	display: block;
	float: left;
	width: 62px;
	text-align: center;
	border: 1px solid #ccc;
	border-radius: 5px;
	color: #738AA0;
}
.l_content .l_content_r .l_content_r_c p:nth-child(3){
	margin-top: 20px;
}
.l_content .l_content_r .l_content_r_c p:nth-child(4){
	color: #AEAEAE;
	margin-top: 10px;
}
.l_content .l_content_r_r{
	float: left;
	margin: 30px;
}
.l_content .l_content_r_r p:nth-child(1) span{
	font-weight: bold;
	color: #F15D53;
	margin: 0 2px;
}
.l_content .l_content_r_r p:nth-child(1) span:nth-child(2){
	font-size: 20px;
}
.l_content .l_content_r_r a{
	display: block;
	width: 100px;
	height: 30px;
	background-color: #F15D53;
	border-radius: 3px;
	color: #fff;
	text-align: center;
	line-height: 30px;
	margin-top: 20px;
}
.w-app-close {
    position: fixed;
    left: 0;
    bottom: 20px;
    height: 140px;
    width: 97px;
    background: url(//s.cctcdn.com/w/1611/i/downloadapp/spring/downloadup-spring.png?v=00fdac) no-repeat;
    cursor: pointer;
    z-index: 99;
}
.page{
    clear: both;
    height: 36px;
    text-align: right;
    font-size: 0;
    margin-top: 20px;
}
.page a{
	font-size: 14px;
    display: inline-block;
    margin-left: 8px;
    padding: 0 14px;
    height: 35px;
    background-color: #FFF;
    border: 1px solid #eee;
    text-align: center;
    color: #666;
    text-align:center;
    line-height: 35px;
}
.now_pages{
	cursor: default;
    background-color: #00AF67;
    border-color: #00AF67;
    color: #fff;
}
.page .select{
	background-color: #58bc58;
	color: #fff;
}
.select1{
	border-color: #58bc58;
}